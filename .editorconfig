root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[README.md]
indent_size = 3
trim_trailing_whitespace = false

[dot_gitconfig.tmpl]
indent_size = 4
indent_style = tab

[.vscode/settings.json]
end_of_line = crlf
indent_size = 4
